@book{GodertHubrichNagelschmidt2014,
 author = {G{\"o}dert, Winfried and Hubrich, Jessica and Nagelschmidt, Matthias},
 year = {2014},
 title = {{Semantic Knowledge Representation for Information Retrieval}},
 publisher = {De Gruyter},
 location = {Berlin},
 abstract = {This book covers the basics of semantic web technologies and indexing languages, and describes their contribution to improve methods of formal knowledge representation and reasoning. The methodologies included combine the specifics of indexing languages, Web representation languages and intersystem relations, and explain their contribution to search functionalities in information retrieval scenarios. An example oriented discussion, considering aspects of conceptual and semantic interoperability in processes of subject querying and knowledge exploration is provided. The book is relevant to information scientists, knowledge workers and indexers. It provides a suitable combination of theoretical foundations and practical applications.  Kap 3 In the previous chapter we discussed the organization of knowledge in terms of indexing and indexing language application from a rather general perspective of KOS modeling. In this chapter, we will focus on the question how knowledge can be actually represented. Therefore, we will present semantic technologies as one prominent technical enabler for knowledge representation and describe the range of modeling possibilities provided by web-based representation languages (i.e. \textit{RDF/RDFS }and \textit{OWL}) as well as application-oriented languages (i.e. \textit{XTM }and \textit{SKOS}).  Kap 5 In the following, we will briefly sketch the so-called ``citation pearl growing'' approach and strategies of modeling multilingual indexing languages. Subsequently, we will introduce how semantic interoperability between indexing languages can be established, as this issue plays an important role when it comes to developing and enhancing indexing languages based on semantic technologies.},
 file = {GoedertHubrichNagelschmidt\_2014-SemanticAuszug:C\:\\Users\\Flöckchen\\Documents\\Citavi 4\\Dissertation\\CitaviFiles\\GoedertHubrichNagelschmidt\_2014-SemanticAuszug.pdf:pdf}
