@book{LemnitzerZinsmeister2006,
 author = {Lemnitzer, Lothar and Zinsmeister, Heike},
 year = {2015},
 title = {{Korpuslinguistik}},
 edition = {3},
 publisher = {Gunter Narr},
 subtitle = {{Eine Einf{\"u}hrung}},
 location = {T{\"u}bingen},
 abstract = {In diesem einf{\"u}hrenden Kapitel definieren wir die zentralen Begriffe Korpus und Korpuslinguistik, wir stellen kurz die linguistischen Str{\"o}mungen vor, die sich auf Korpora als empirische Daten beziehen, und geben einen {\"U}berblick {\"u}ber den Rest des Buches.  Nach dem Durcharbeiten dieses Kapitels werden Sie wissen, wie in zwei gro{\ss}en Str{\"o}mungen der Linguistik, in der generativen Grammatik und im Kontextualismus, mit Sprachdaten umgegangen wird. Sie werden die unterschiedlichen erkenntnistheoretischen Positionen, auf denen beide Str{\"o}mungen aufbauen, unterscheiden und Sie werden erkl{\"a}ren k{\"o}nnen, welches Verh{\"a}ltnis sie jeweils zu Sprachdaten haben und welche Arten von Sprachdaten sie in ihrer Forschung verwenden. Sie werden verstehen, warum Noam Chomsky j{\"u}ngst in einem Interview behauptete, dass es so etwas wie Korpuslinguistik nicht gebe. Sie werden aber auch gesehen haben, warum es sich dennoch lohnt, Korpuslinguistik zu betreiben. Au{\ss}erdem werden Sie drei unterschiedliche Ans{\"a}tze, Korpuslinguistik zu betreiben, kennengelernt haben. Sie werden Ihre eigenen Arbeiten so besser einordnen k{\"o}nnen.  Abstract: In diesem Kapitel stellen wir die wichtigsten Merkmale linguistischer Korpora vor und grenzen diese Datensammlungen von anderen linguistischen Datensammlungen ab. Wir unterscheiden bei linguistischen Korpora drei Datenebenen: Prim{\"a}rdaten, Metadaten und linguistische Annotation. Wir gehen in diesem Kapitel n{\"a}her auf die Prim{\"a}rdaten und Metadatenvon ein. Die linguistische Annotation behandeln wir ausf{\"u}hrlich in Kapitel 4. Wir versuchen, Antworten auf drei schwierige methodologische Fragen zu finden: wie verh{\"a}lt sich mein Korpus zum Gegenstand, den ich eigentlich untersuchen will? Was mache ich, wenn ich im Korpus etwas nicht finde,was ich suche und beschreiben m{\"o}chte, und umgekehrt: was mache ich, wenn ich etwas finde, was es nach einer bestimmten Sprachtheorie eigentlich gar nicht geben d{\"u}rfte. Schlie{\ss}lich gehen wir auf die hochaktuelle Frage ein, ob und wie man das World Wide Web als Quelle von Sprachdaten verwenden kann.}
}


