@incollection{Pitti2004,
 author = {Pitti, Daniel},
 title = {{Designing Sustainable Projects and Publications}},
 url = {http://digitalhumanities.org/companion/view?docId=blackwell/9781405103213/9781405103213.xml\&chunk.id=ss1-5-1},
 urldate = {2016-09-09},
